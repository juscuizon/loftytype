mixin navigation-links
	ul
		li: a.text(href="#") SERVICES
		li: a.text(href="#") ABOUT US
		li: a.non-text(href="#"): img.logo(src="{{site.baseurl}}/assets/img/logo.svg")
		li: a.text(href="#") BLOG
